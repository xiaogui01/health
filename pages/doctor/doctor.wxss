.container{
  font-family: '微软雅黑';
}
.search{
  width: 100%;
}
.search-left{
  flex: 8;
  background: #ffffff;
  width: 78%;
  border: 1px solid #efefef;
  border-radius: 5px;
  margin-top: 10px;
  margin-left: 8px;
  float: left;
}
.search-right{
  margin-top: 10px;
  margin-right:4px;
  width: 16%;
  float: right;
}
.search-right button{
 text-align: center;
  font-size: 26rpx;
  color: #ffffff;
  background-color: #4EA3E7;
  height: 38px;
}
.search-left input{
  display: inline-block;
  height: 70rpx;
  font-size: 26rpx;
  width: 200px;
}
.search-placeholder{
  color: #bfbfbf;
  line-height: 20rpx;
  font-family: '微软雅黑';
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.book_time{
  font-size: 16px;
  width: 100%;
  height: 100px;
  text-align: center;
  border-bottom: 1px solid #efefef;
  border-left: 1px solid #efefef;
  margin-left: -1px;
  float: left;
}
.line_d {
	height: 69px;
	border-bottom: 1px solid #efefef;
}
.line_dn{
  padding-top: 10px;
  text-indent: 1rem;
	padding-right: 30rpx;
  height: 50px;
	line-height: 3;
}
.line_d:before {
	content: '>';
	float: right;
  line-height: 3;
	color: #aaa;
  padding-right: 10px;
}
.book_doctor{
  margin-top: 150px;
}

.book_img{
  width: 90rpx;
  height: 90rpx;
  line-height: 4;
  display: inline-block;
  float: left;
  margin-left: 10px;
}
.bookImg{
  text-align: center;
  width: 15%;
  float: left;
}
.bookDn{
  width: 60%;
  float: left;
}
.book_dn{
  height: 21px;
  line-height: 21px; 
}
.timeH{
  font-family: '微软雅黑';
  margin: 12px 0;
  text-align: center;
  font-size: 16px;
  color: #4EA3E7;
}
.timeDay{
  width: 25%;
  height: 50px;
  line-height: 50px;
  float: left;
  border-top: 1px solid #efefef;
  border-right: 1px solid #efefef;
  margin-right: -1px;
  font-family: '微软雅黑';
}
.active{
  font-weight: bold;
  color: #ffffff;
  background-color: #4EA3E7;
  background-image: url('../../images/dui.png');
  background-repeat:no-repeat;
}
.bookSelect{
  width: 10px;
  height: 10px;
}